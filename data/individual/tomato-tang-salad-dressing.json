    "ingredients": [
        "1 cup chopped fresh tomato",
        "1/3 cup red wine vinegar",
        "1 dash Worcestershire sauce",
        "1 teaspoon ketchup",
        "2 teaspoons Dijon mustard",
        "1 tablespoon minced fresh basil",
        "1 tablespoon minced fresh thyme"
