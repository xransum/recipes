{
    "directions": [
        "Blend almond milk, banana, strawberries, ice cubes, protein powder, vanilla extract, honey, ground flax seeds, ground chia seeds, and cinnamon in a blender until smooth."
    ],
    "ingredients": [
        "1 cup almond milk",
        "1 chopped banana, frozen",
        "3/4 cup strawberries",
        "3 ice cubes",
        "1 scoop vanilla protein powder",
        "1 teaspoon vanilla extract",
        "1 teaspoon honey",
        "1 teaspoon ground flax seed",
        "1 teaspoon ground chia seeds",
        "1/2 teaspoon ground cinnamon"
    ],
