    "directions": [
        "Combine wine, brandy, triple sec, peach schnapps, lemonade concentrate, lemon slices, lime slices, orange slices, and maraschino cherries in a pitcher; chill at least 24 hours before serving."
    ],
    "ingredients": [
        "1 (750 milliliter) bottle Spanish red wine",
        "1/2 cup brandy",
        "1/2 cup triple sec",
        "1/2 cup peach schnapps",
        "1/4 cup frozen lemonade concentrate, thawed",
        "1 lemon, sliced",
        "1 lime, sliced",
        "1 orange, sliced",
        "1 (5 ounce) jar maraschino cherries"
    ],
