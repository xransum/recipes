    "directions": [
        "Stir tomato sauce, tomato paste, vegetable oil, garlic, salt, oregano, sugar, and pepper together in a bowl; refrigerate until chilled completely, at least 15 minutes."
    ],
    "ingredients": [
        "1 (8 ounce) can tomato sauce",
        "1 (6 ounce) can tomato paste",
        "1 tablespoon vegetable oil",
        "1 clove garlic, minced",
        "1 teaspoon salt",
        "1/2 teaspoon oregano",
        "1/2 teaspoon white sugar",
        "1/8 teaspoon ground black pepper"
    ],
