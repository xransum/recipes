{
  "title": "Rum Butter",
  "directions": [
    "Blend brown sugar, butter, rum, nutmeg, and cinnamon in a food processor or blender until evenly combined."
  ],
  "ingredients": [
    "1 pound brown sugar",
    "1 cup butter, melted",
    "1/2 cup dark rum",
    "1/2 teaspoon ground nutmeg",
    "1/2 teaspoon ground cinnamon"
  ],
