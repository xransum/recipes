{
  "title": "The BEST Margarita Mix !!!",
  "directions": [
    "Blend tequila, triple sec, lime juice, honey, and ice in a blender until smooth."
  ],
  "ingredients": [
    "1/2 cup tequila",
    "1/2 cup triple sec",
    "1/4 cup freshly squeezed lime juice",
    "2 tablespoons honey",
    "4 cups ice"
  ],
