{
  "title": "Floradora",
  "directions": [
    "Put the ice into a 12-ounce collins glass, add remaining ingredients and stir gently."
  ],
  "ingredients": [
    "1 cup crushed ice",
    "2 ounces gin",
    "3 tablespoons lime juice",
    "1/2 teaspoon superfine sugar",
    "1 tablespoon grenadine or rasberry syrup",
    "2 ounces club soda or ginger ale"
  ],
