{
  "title": "Spicy Fry Sauce",
  "directions": [
    "Stir together mayonnaise and ketchup; season with cayenne, onion powder, pepper, and salt."
  ],
  "ingredients": [
    "1/4 cup mayonnaise",
    "1/4 cup ketchup",
    "1/8 teaspoon cayenne pepper",
    "1/4 teaspoon onion powder",
    "1/8 teaspoon ground black pepper",
    "1/8 teaspoon salt"
  ],
