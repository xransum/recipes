{
  "title": "Orange Mocha",
  "directions": [
    "Stir coffee, orange juice, milk, sugar, and cocoa powder together in a mug until the sugar and cocoa dissolve."
  ],
  "ingredients": [
    "1 cup brewed coffee",
    "2 tablespoons orange juice",
    "2 tablespoons milk",
    "1 tablespoon white sugar",
    "1 tablespoon unsweetened cocoa powder"
  ],
