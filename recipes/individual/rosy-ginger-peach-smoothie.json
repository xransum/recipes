{
  "title": "Rosy Ginger Peach Smoothie",
  "directions": [
    "Place peach slices, yogurt, milk, strawberries, honey, and ground ginger in a blender; cover and blend until smooth."
  ],
  "ingredients": [
    "1 fresh peach, sliced",
    "1 (6 ounce) container strawberry-flavored yogurt",
    "1/2 cup milk",
    "1/2 cup strawberries",
    "1 tablespoon honey",
    "1/8 teaspoon ground ginger"
  ],
  "lang": "en-US",
  "tags": [],
  "source": "All Recipes",
  "sourceUrl": "http://allrecipes.com/recipe/238004/rosy-ginger-peach-smoothie/"
}