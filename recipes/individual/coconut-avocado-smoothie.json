{
  "title": "Coconut Avocado Smoothie",
  "directions": [
    "Combine avocado, yogurt, milk, cream of coconut, and ice cubes in a blender; blend until smooth."
  ],
  "ingredients": [
    "1 Hass avocado, diced",
    "1/2 cup low-fat vanilla yogurt",
    "1/2 cup whole milk",
    "1/4 cup cream of coconut",
    "8 ice cubes"
  ],
