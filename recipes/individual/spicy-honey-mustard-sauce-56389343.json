{
  "title": "Spicy Honey Mustard Sauce",
  "directions": [
    "In a medium bowl, whisk all ingredients together."
  ],
  "ingredients": [
    "1 cup Dijon mustard",
    "1/2 cup mayonnaise",
    "1/2 cup honey",
    "1/2 teaspoon kosher salt",
    "1/4 teaspoon cayenne pepper",
    "1/4 teaspoon dry mustard powder"
  ],
