    ],
    "ingredients": [
        "1/2 cup paprika",
        "1/2 cup chili powder",
        "1/4 cup brown sugar",
        "1/4 cup white sugar",
        "2 teaspoons celery seed",
        "2 teaspoons ground black pepper",
        "2 teaspoons cayenne pepper",
        "2 teaspoons dry brown mustard",
        "2 teaspoons garlic powder",
        "1 teaspoon ground cumin",
        "1 teaspoon salt"
