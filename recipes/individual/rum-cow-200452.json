    "ingredients": [
        "1 1/2 ounces light rum",
        "2 drops vanilla",
        "Dash Angostura bitters",
        "2 teaspoons sugar",
        "1 cup milk",
        "Nutmeg",
        "3 or 4 ice cubes"
