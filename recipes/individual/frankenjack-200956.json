{
  "title": "Frankenjack",
  "directions": [
    "Combine all ingredients, except the cherry, in a mixing glass and stir well. Strain into a cocktail glass and drop in cherry."
  ],
  "ingredients": [
    "1 ounce gin",
    "1 ounce dry vermouth",
    "1/2 ounce apricot brandy",
    "1/2 ounce Cointreau",
    "3 or 4 ice cubes",
    "Maraschino cherry"
  ],
