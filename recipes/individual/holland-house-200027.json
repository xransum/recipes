{
  "title": "Holland House",
  "directions": [
    "Combine all ingredients in a mixing glass and stir well. Strain into a cocktail glass."
  ],
  "ingredients": [
    "1 1/2 ounces gin",
    "1/2 ounce dry vermouth",
    "1 tablespoon lemon juice",
    "1 slice pineapple, coarsely chopped",
    "1/4 teaspoon maraschino",
    "3 or 4 ice cubes"
  ],
