{
  "title": "Cake and Ice Cream",
  "directions": [
    "Blend ice cream, milk, ice, vodka, and sugar together in a blender until smooth."
  ],
  "ingredients": [
    "4 scoops vanilla ice cream",
    "1/2 cup milk",
    "1/2 cup crushed ice",
    "3 fluid ounces cake-flavored vodka",
    "1 tablespoon white sugar"
  ],
