{
  "title": "Spicy Sushi Dipping Sauce",
  "directions": [
    "Stir soy sauce, chile-garlic sauce, sesame oil, and garlic powder together in a small bowl; add lemon slice."
  ],
  "ingredients": [
    "2 tablespoons soy sauce",
    "1/2 teaspoon chile-garlic sauce (such as Sriracha)",
    "1/4 teaspoon sesame oil",
    "1 pinch garlic powder (optional)",
    "1 thin slice lemon (optional)"
  ],
  "lang": "en-US",
