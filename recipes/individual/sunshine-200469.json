{
  "title": "Sunshine",
  "directions": [
    "Combine all ingredients, except the orange peel, in a mixing glass and stir well. Strain into a cocktail glass and drop in the orange peel."
  ],
  "ingredients": [
    "1 1/2 ounces gin",
    "1/2 ounce sweet vermouth",
    "2 dashes Angostura bitters",
    "3 or 4 ice cubes",
    "Twist orange peel"
  ],
