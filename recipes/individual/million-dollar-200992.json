{
  "title": "Million Dollar",
  "directions": [
    "Combine all ingredients in a cocktail shaker and shake vigorously. Strain into a cocktail glass."
  ],
  "ingredients": [
    "1 1/2 ounces gin",
    "3/4 ounce sweet vermouth",
    "1 teaspoon grenadine",
    "1 tablespoon pineapple juice",
    "1 egg white",
    "3 or 4 ice cubes"
  ],
