{
  "title": "Mango Chia Smoothie",
  "directions": [
    "Blend almond milk, spinach, mango, banana, ice cubes, chia seeds, honey, and turmeric in a blender until smooth, about 30 seconds."
  ],
  "ingredients": [
    "1 1/2 cups almond milk",
    "1 1/2 cups fresh spinach (optional)",
    "1 mango, peeled and cubed",
    "1 banana",
    "1/2 cup ice cubes",
    "2 tablespoons chia seeds",
    "1 teaspoon honey",
    "1/2 teaspoon ground turmeric"
  ],
  "lang": "en-US",
  "tags": [],
  "source": "All Recipes",
  "sourceUrl": "http://allrecipes.com/recipe/255456/mango-chia-smoothie/"
