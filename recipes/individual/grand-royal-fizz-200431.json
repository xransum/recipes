{
  "title": "Grand Royal Fizz",
  "directions": [
    "Combine all ingredients except the club soda in a cocktail shaker and shake vigorously. Strain into a highball glass and fill with soda."
  ],
  "ingredients": [
    "3 ounces gin",
    "2 tablespoons orange juice",
    "2 tablespoons lemon juice",
    "1 teaspoon superfine sugar",
    "1/2 teaspoon maraschino",
    "2 teaspoons heavy cream",
    "3 or 4 ice cubes",
    "Club soda"
  ],
