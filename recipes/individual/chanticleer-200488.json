{
  "title": "Chanticleer",
  "directions": [
    "Combine all ingredients in a cocktail shaker and shake vigorously. Strain into a cocktail glass."
  ],
  "ingredients": [
    "2 to 3 ounces gin",
    "2 tablespoons lemon juice",
    "1 tablespoon raspberry syrup",
    "1 egg white",
    "3 or 4 ice cubes"
  ],
