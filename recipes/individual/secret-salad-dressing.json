    "ingredients": [
        "1/2 cup ketchup",
        "1 cup vegetable oil",
        "1/4 cup mayonnaise",
        "1/4 cup white vinegar",
        "1/4 cup white sugar",
        "1/4 cup diced onion",
        "1 teaspoon salt",
        "1/4 teaspoon pepper"
