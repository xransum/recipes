{
  "title": "Nutty Avocado",
  "directions": [
    "Blend pistachios and almonds in a blender until ground; add coconut milk, ice cubes, avocado, honey, and saffron. Blend until smooth."
  ],
  "ingredients": [
    "1 tablespoon shelled pistachios",
    "1 tablespoon almonds",
    "3/4 cup unsweetened coconut milk",
    "5 cubes ice cubes",
    "1/2 large avocado, peeled and pitted",
    "1 tablespoon honey",
    "1 pinch saffron threads (optional)"
  ],
  "lang": "en-US",
  "tags": [],
  "source": "All Recipes",
  "sourceUrl": "http://allrecipes.com/recipe/235971/nutty-avocado/"
}