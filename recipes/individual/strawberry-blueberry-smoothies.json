{
  "title": "Strawberry Blueberry Smoothies",
  "directions": [
    "Blend almond milk, strawberries, blueberries, yogurt, flax seed oil, and agave nectar in a blender until smooth."
  ],
  "ingredients": [
    "1/2 cup almond milk",
    "1/2 cup frozen strawberries",
    "1/2 cup frozen blueberries",
    "1/2 cup low-fat plain yogurt",
    "1 teaspoon flax seed oil",
    "1 teaspoon agave nectar"
  ],
  "lang": "en-US",
  "tags": [],
  "source": "All Recipes",
  "sourceUrl": "http://allrecipes.com/recipe/223140/strawberry-blueberry-smoothies/"
}