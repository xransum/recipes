{
  "title": "Yummy Strawberry Shake",
  "directions": [
    "In a blender combine strawberries, ice, milk, ice cream and sugar. Blend until smooth"
  ],
  "ingredients": [
    "4 cups strawberries, hulled",
    "2 cups ice cubes",
    "2 cups milk",
    "4 cups strawberry ice cream",
    "2 teaspoons white sugar"
  ],
