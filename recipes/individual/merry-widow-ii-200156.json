{
  "title": "Merry Widow II",
  "directions": [
    "Combine all ingredients, except peel, in a mixing glass and stir well. Strain into a cocktail glass; drop in peel."
  ],
  "ingredients": [
    "1 to 1 1/2 ounces gin",
    "1 to 1 1/2 ounces dry vermouth",
    "2 dashes Pernod",
    "2 dashes Bénédictine",
    "Dash Peychaud's or orange bitters",
    "3 or 4 ice cubes",
    "Twist lemon peel"
  ],
