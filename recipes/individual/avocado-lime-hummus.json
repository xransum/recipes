    "ingredients": [
        "2 cups cooked garbanzo beans",
        "1 avocado, peeled and pitted",
        "1/4 cup water",
        "1/4 cup sesame seeds",
        "1 lime, zested and juiced",
        "2 tablespoons olive oil",
        "2 tablespoons chopped fresh rosemary"
