{
  "title": "Easiest Egg Nog",
  "directions": [
    "Blend together eggs, sugar, milk, vanilla and nutmeg. Serve chilled."
  ],
  "ingredients": [
    "2 eggs, beaten",
    "3 tablespoons sugar",
    "2 1/3 cups low-fat milk",
    "1 teaspoon vanilla extract",
    "1 dash ground nutmeg"
  ],
  "lang": "en-US",
