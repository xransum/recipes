    "ingredients": [
        "1 medium head cabbage, chopped",
        "6 Roma (plum) tomatoes, diced",
        "1 red onion, diced",
        "1 yellow onion, diced",
        "4 jalapeno peppers, diced",
        "2 bunches cilantro, chopped",
        "3 limes, juiced",
        "2 teaspoons chopped garlic",
        "2 teaspoons salt"
