{
  "title": "Preakness",
  "directions": [
    "Combine all ingredients except the lemon peel in a mixing glass and stir well. Strain into a cocktail glass and drop in the lemon peel."
  ],
  "ingredients": [
    "1 1/2 ounces whiskey",
    "3/4 ounce sweet vermouth",
    "1/2 teaspoon Bénédictine",
    "Dash Angostura bitters",
    "3 or 4 ice cubes",
    "Twist lemon peel"
  ],
