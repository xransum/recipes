{
  "title": "Tropical Strawberry",
  "directions": [
    "In a blender, blend the strawberries, pineapple, milk, yogurt, sugar, and ice until smooth."
  ],
  "ingredients": [
    "1 1/2 cups frozen strawberries",
    "1 cup frozen pineapple chunks",
    "1/2 cup milk",
    "1 1/2 cups yogurt",
    "2 tablespoons white sugar",
    "1 cup crushed ice"
  ],
