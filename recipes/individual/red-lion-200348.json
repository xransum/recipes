{
  "title": "Red Lion",
  "directions": [
    "Combine all ingredients except the lemon peel in a cocktail shaker and shake vigorously. Strain into a cocktail glass and drop in the peel."
  ],
  "ingredients": [
    "2 ounces Grand Marnier",
    "1 ounce gin",
    "1 tablespoon orange juice",
    "1 tablespoon lemon juice",
    "3 or 4 ice cubes",
    "Twist lemon peel"
  ],
