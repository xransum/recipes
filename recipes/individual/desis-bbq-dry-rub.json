    "ingredients": [
        "1 cup packed brown sugar",
        "1/2 cup paprika",
        "2 tablespoons coarsely ground black pepper",
        "2 tablespoons cayenne pepper",
        "2 tablespoons coarse salt",
        "1 tablespoon garlic powder",
        "1 1/2 teaspoons ground cumin"
