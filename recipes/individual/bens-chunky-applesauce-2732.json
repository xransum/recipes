{
  "title": "Ben's Chunky Applesauce",
  "directions": [
    "Combine all ingredients in heavy 3-quart saucepan. Cover and cook over medium-low until apples are tender, stirring occasionally, about 45 minutes."
  ],
  "ingredients": [
    "6 pounds green apples, peeled, quartered, cored",
    "1 cup raisins",
    "1/2 cup dried currants",
    "1/2 cup apple juice",
    "1/4 cup sugar",
    "1 teaspoon ground cinnamon",
    "1/4 teaspoon ground nutmeg"
  ],
