    ],
    "ingredients": [
        "1/2 cup superfine sugar",
        "1/3 cup gold tequila",
        "1/3 cup triple sec",
        "2 large kiwis, peeled",
        "1 cup fresh lime juice",
        "2 cups small ice cubes"
