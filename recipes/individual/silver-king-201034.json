{
  "title": "Silver King",
  "directions": [
    "Combine all ingredients in a cocktail shaker and shake vigorously. Strain into a large cocktail glass."
  ],
  "ingredients": [
    "1 1/2 ounces gin",
    "2 tablespoons lemon juice",
    "2 dashes sugar syrup",
    "2 dashes orange bitters",
    "1 egg white",
    "3 or 4 ice cubes"
  ],
