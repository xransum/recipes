{
  "title": "Banana Anna",
  "directions": [
    "In a blender combine banana, milk, honey and nutmeg. Blend until smooth."
  ],
  "ingredients": [
    "1 banana",
    "1 1/2 cups milk",
    "1 tablespoon honey",
    "1/4 teaspoon ground nutmeg"
  ],
