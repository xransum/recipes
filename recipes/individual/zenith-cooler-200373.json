{
  "title": "Zenith Cooler",
  "directions": [
    "Put the ice cubes in a 10-ounce highball glass and pour in the gin and pineapple juice. Fill up with club soda and stir. Decorate with pineapple stick."
  ],
  "ingredients": [
    "3 or 4 ice cubes",
    "2-3 ounces gin",
    "1 tablespoon pineapple juice",
    "Chilled club soda",
    "Pineapple stick"
  ],
