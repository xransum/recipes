  "directions": [
    "Stir mayonnaise, mustard, horseradish, and Worcestershire sauce together until smooth."
  ],
  "ingredients": [
    "1 1/2 cups mayonnaise",
    "3 tablespoons Dijon mustard",
    "1 1/2 teaspoons prepared horseradish",
    "3/4 teaspoon Worcestershire sauce"
  ],
