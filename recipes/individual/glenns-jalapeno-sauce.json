{
  "title": "Glenn's Jalapeno Sauce",
  "directions": [
    "Blend sour cream, jalapeno peppers, green onions, cilantro, garlic powder, and black pepper together in a blender until smooth."
  ],
  "ingredients": [
    "1 (8 ounce) container sour cream",
    "2 jalapeno peppers, seeded and chopped",
    "2 green onions, chopped",
    "2 tablespoons chopped fresh cilantro",
    "1/2 teaspoon garlic powder",
    "1/2 teaspoon ground black pepper"
  ],
  "lang": "en-US",
  "tags": [],
  "source": "All Recipes",
  "sourceUrl": "http://allrecipes.com/recipe/239727/glenns-jalapeno-sauce/"
}