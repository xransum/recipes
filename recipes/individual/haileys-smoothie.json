{
  "title": "Hailey's Smoothie",
  "directions": [
    "In a blender, combine the kiwis, frozen bananas, blueberries, yogurt, crushed ice, honey and almond extract. Blend until smooth."
  ],
  "ingredients": [
    "3 kiwis, peeled and chopped",
    "2 frozen bananas, peeled and chopped",
    "1 cup blueberries",
    "1 cup plain yogurt",
    "1 1/2 cups crushed ice",
    "3 tablespoons honey",
    "1/4 teaspoon almond extract"
  ],
  "lang": "en-US",
  "tags": [],
  "source": "All Recipes",
  "sourceUrl": "http://allrecipes.com/recipe/18941/haileys-smoothie/"
}