{
  "title": "Watermelon Cooler Slushy",
  "directions": [
    "Place watermelon and ice into a blender. Pour in lime juice, sugar, and salt. Blend until smooth."
  ],
  "ingredients": [
    "4 cups cubed seedless watermelon",
    "10 ice cubes",
    "1/3 cup fresh lime juice",
    "1/4 cup white sugar",
    "1/8 teaspoon salt"
  ],
  "lang": "en-US",
