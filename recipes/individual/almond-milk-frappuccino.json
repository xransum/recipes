{
  "title": "Almond Milk Frappuccino",
  "directions": [
    "Blend ice, almond milk, espresso, and honey in a blender until smooth."
  ],
  "ingredients": [
    "1 cup ice",
    "1 cup vanilla-flavored almond milk",
    "2 shots brewed espresso, chilled",
    "1 teaspoon honey, or to taste"
  ],
