    ],
    "ingredients": [
        "2 cups mayonnaise",
        "1/2 cup water",
        "1 teaspoon white sugar",
        "1 teaspoon paprika",
        "1 teaspoon garlic juice",
        "1 tablespoon ketchup",
        "1 teaspoon ground ginger",
        "1 teaspoon hot pepper sauce",
        "1 teaspoon ground mustard",
        "1/4 teaspoon salt",
        "3/4 teaspoon ground white pepper"
    ],
