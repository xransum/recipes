{
  "title": "KCB",
  "directions": [
    "Combine all ingredients, except the peel, in a mixing glass and stir well. Strain into a cocktail glass and add the peel."
  ],
  "ingredients": [
    "1 1/2 ounces gin",
    "1/2 ounce kirsch",
    "Dash apricot brandy",
    "Dash lemon juice",
    "3 or 4 ice cubes",
    "Twist lemon peel"
  ],
