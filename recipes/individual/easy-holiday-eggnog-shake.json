{
  "title": "Easy Holiday Eggnog Shake",
  "directions": [
    "Combine banana, frozen yogurt, milk, eggnog, ice cubes, and malted milk powder; blend until smooth."
  ],
  "ingredients": [
    "1 large banana",
    "1/2 cup vanilla frozen yogurt",
    "1/2 cup milk",
    "1/2 cup low-fat eggnog",
    "4 large ice cubes",
    "1 teaspoon malted milk powder (optional)"
  ],
  "lang": "en-US",
  "tags": [],
  "source": "All Recipes",
  "sourceUrl": "http://allrecipes.com/recipe/256423/easy-holiday-eggnog-shake/"
}