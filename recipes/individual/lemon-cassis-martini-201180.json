    "ingredients": [
        "3/4 cup vodka",
        "4 teaspoons fresh lemon juice",
        "2 teaspoons sugar",
        "1/2 teaspoon grated lemon peel",
        "1 tablespoon cr\u00e8me de cassis",
        "Ice cubes",
        "Lemon peel twists"
