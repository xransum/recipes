{
  "title": "Mango Mayhem Peanut Butter Smoothie",
  "directions": [
    "Blend all ingredients except ice in blender until smooth.",
    "Add ice; blend until thickened.",
    "Serve immediately."
  ],
  "ingredients": [
    "1/2 cup skim milk",
    "1/2 cup plain low fat Greek-style yogurt",
    "2 tablespoons KRAFT Smooth Peanut Butter",
    "1 tablespoon honey",
    "1/2 cup frozen chopped mango",
    "4 ice cubes"
  ],
