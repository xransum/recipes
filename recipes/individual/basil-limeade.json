{
  "title": "Basil Limeade",
  "directions": [
    "Place grapes, lime, dates, basil, and ice, respectively, in a blender; blend on high until smooth."
  ],
  "ingredients": [
    "1 cup seedless green grapes",
    "1/8 lime, including rind",
    "2 pitted dates",
    "1 cup fresh basil leaves",
    "1 cup ice"
  ],
