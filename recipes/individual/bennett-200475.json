{
  "title": "Bennett",
  "directions": [
    "Combine all ingredients in a cocktail shaker and shake vigorously. Strain into a cocktail glass."
  ],
  "ingredients": [
    "2 ounces gin",
    "2 tablespoons lime juice",
    "1 teaspoon superfine sugar",
    "2 dashes Angostura or orange bitters",
    "3 or 4 ice cubes"
  ],
