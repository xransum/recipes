{
  "title": "Fairy Belle",
  "directions": [
    "Combine all ingredients in a cocktail shaker and shake vigorously. Strain into a cocktail glass."
  ],
  "ingredients": [
    "2 ounces gin",
    "1/2 ounce apricot brandy",
    "1 teaspoon grenadine",
    "1 egg white",
    "3 or 4 ice cubes"
  ],
