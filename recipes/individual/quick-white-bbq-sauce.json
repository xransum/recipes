{
  "title": "Quick White BBQ Sauce",
  "directions": [
    "Stir mayonnaise, horseradish, vinegar, sugar, salt, black pepper, and cayenne pepper together in a bowl."
  ],
  "ingredients": [
    "2 cups mayonnaise",
    "1/2 cup prepared horseradish",
    "1/4 cup cider vinegar",
    "2 tablespoons white sugar",
    "1 tablespoon kosher salt",
    "1 1/2 teaspoons ground black pepper",
    "1/2 teaspoon cayenne pepper"
  ],
  "lang": "en-US",
  "tags": [],
  "source": "All Recipes",
