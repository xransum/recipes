    "directions": [
        "Blend vegetable oil, sugar, vinegar, ketchup, salt, celery seed, paprika, and dried minced onion in a blender until smooth."
    ],
    "ingredients": [
        "1 cup vegetable oil",
        "3/4 cup white sugar",
        "1/3 cup vinegar",
        "1/3 cup ketchup",
        "1 teaspoon salt",
        "1 teaspoon celery seed",
        "1 teaspoon paprika",
        "1 tablespoon dried minced onion"
    ],
