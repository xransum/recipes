{
  "title": "Orange Vanilla Blend",
  "directions": [
    "In a blender combine frozen banana, tofu, orange juice, honey and vanilla. Blend until smooth."
  ],
  "ingredients": [
    "1 frozen banana, peeled and chopped",
    "1 (12 ounce) package soft silken tofu",
    "2 cups orange juice",
    "1 tablespoon honey",
    "1/2 teaspoon vanilla extract"
  ],
  "lang": "en-US",
