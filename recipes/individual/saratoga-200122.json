{
  "title": "Saratoga",
  "directions": [
    "Put lemon juice, sugar and bitters in a tall glass with ice cubes, stir to mix and fill with ginger ale."
  ],
  "ingredients": [
    "2 tablespoons lemon juice",
    "1/2 teaspoon superfine sugar",
    "2 dashes Angostura bitters",
    "2 or 3 ice cubes",
    "Ginger ale"
  ],
