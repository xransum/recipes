{
  "title": "Coconut Raspberry Smoothie",
  "directions": [
    "Place bananas, raspberries, pecans, coconut oil, flax meal, and date in a blender; add water. Blend until smooth."
  ],
  "ingredients": [
    "2 bananas, broken into chunks",
    "1 cup frozen raspberries",
    "2 tablespoons pecan halves",
    "1 tablespoon coconut oil",
    "1 tablespoon flax seed meal",
    "1 date, pitted",
    "16 fluid ounces water"
  ],
  "lang": "en-US",
  "tags": [],
  "source": "All Recipes",
  "sourceUrl": "http://allrecipes.com/recipe/244182/coconut-raspberry-smoothie/"
}