{
  "title": "Texas Chocolate Frosting",
  "directions": [
    "Melt margarine over a low heat. Gradually beat in cocoa, sugar, milk and vanilla, mix well."
  ],
  "ingredients": [
    "1/2 cup margarine",
    "4 tablespoons cocoa powder",
    "4 cups confectioners' sugar",
    "3 tablespoons milk",
    "1 teaspoon vanilla extract"
  ],
  "lang": "en-US",
