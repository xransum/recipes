{
  "title": "Jamaica Glow",
  "directions": [
    "Combine all ingredients in a cocktail shaker and shake vigorously. Strain into a cocktail glass."
  ],
  "ingredients": [
    "1 1/2 ounces gin",
    "1 teaspoon Jamaica rum",
    "1/2 ounce dry red wine",
    "1/2 ounce orange juice",
    "3 or 4 ice cubes"
  ],
