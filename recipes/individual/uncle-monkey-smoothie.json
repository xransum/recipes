{
  "title": "Uncle Monkey Smoothie",
  "directions": [
    "Blend bananas, yogurt, ice cubes, peanut butter, milk, and sugar together in a blender until smooth."
  ],
  "ingredients": [
    "2 bananas, cut into small chunks",
    "1/4 cup vanilla yogurt",
    "5 ice cubes",
    "3 tablespoons peanut butter",
    "2 tablespoons milk",
    "1 tablespoon white sugar"
  ],
  "lang": "en-US",
