{
  "title": "Virgin Pina Colada",
  "directions": [
    "In an electric blender, blend ice, pineapple juice, milk, cream, and sugar. Blend until smooth."
  ],
  "ingredients": [
    "1 cup ice",
    "1 1/4 cups pineapple juice",
    "1/2 cup milk",
    "1/2 cup heavy cream",
    "2 tablespoons white sugar"
  ],
  "lang": "en-US",
