{
  "title": "Blueberry and Chia Quinoa",
  "directions": [
    "Stir soy milk, water, quinoa, blueberries, chia seeds, and honey together in a slow cooker.",
    "Cook on Low 6 to 8 hours."
  ],
  "ingredients": [
    "4 cups soy milk",
    "4 cups water",
    "2 cups quinoa",
    "2 cups blueberries",
    "1/3 cup chia seeds",
    "1/3 cup honey"
  ],
