{
  "title": "Snicker",
  "directions": [
    "Combine all ingredients in a cocktail shaker and shake vigorously. Strain into a cocktail glass."
  ],
  "ingredients": [
    "1 1/2 ounces gin",
    "3/4 ounce dry vermouth",
    "1/2 teaspoon maraschino",
    "Dash orange bitters",
    "1 teaspoon sugar syrup",
    "1 egg white",
    "3 or 4 ice cubes"
  ],
