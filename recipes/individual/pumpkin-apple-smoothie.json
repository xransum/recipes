{
    "directions": [
        "Blend almond milk, apple, banana, pumpkin puree, dates, ice, chia seeds, oats, hemp seeds, pumpkin pie spice, molasses, honey, and vanilla extract in a blender until smooth."
    ],
    "ingredients": [
        "1 1/2 cups almond milk",
        "1 apple, cored and chopped",
        "1/2 sliced frozen banana",
        "1/4 cup frozen pumpkin puree",
        "3 dates, pitted",
        "3 cubes ice (optional)",
        "2 tablespoons ground chia seeds",
        "2 tablespoons large flake oats",
        "1 tablespoon hemp seeds",
        "1 1/2 teaspoons pumpkin pie spice",
        "1 teaspoon molasses",
        "1 teaspoon honey (optional)",
        "1/4 teaspoon pure vanilla extract"
    ],
    "language": "en-US",
    "source": "allrecipes.com",
    "tags": [],
    "title": "Pumpkin Apple Smoothie",
    "url": "http://allrecipes.com/recipe/240506/pumpkin-apple-smoothie/"
}
