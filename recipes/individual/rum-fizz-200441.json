{
  "title": "Rum Fizz",
  "directions": [
    "Combine all ingredients except the soda in a cocktail shaker and shake vigorously. Strain into a highball glass and fill with soda."
  ],
  "ingredients": [
    "1 1/2 ounces rum",
    "3/4 ounces cherry brandy",
    "1/2 teaspoon sugar",
    "2 tablespoons lemon juice",
    "3 or 4 ice cubes",
    "Club soda"
  ],
